var img = new Array();
img[0] = new Image();
img[0].src = "<%= image_path('white_heart.png') %>";
img[1] = new Image();
img[1].src = "<%= image_path('heart.png') %>";

var cnt = 0;

function LinkClick() {
  if (cnt == 1){
    cnt = 0;
  }
  else{
    cnt++;
  }
  document.getElementById("heart_img").src = img[cnt].src;
}
